// This only outputs the type declaration file, esbuild does the rest.
{
  "compilerOptions": {
    "module": "amd", // https://github.com/Microsoft/TypeScript/issues/27117
    "target": "ES2020",
    "declaration": true,
    "emitDeclarationOnly": true,
    "outFile": "dist/q.d.ts"
  },
  "include": ["src/*"]
}
